{
  "name": "JPTax Extension",
  "description": "Chrome extension for National Tax Agency Japan Websites",
  "version": "1.2",
  "manifest_version": 2,
  "permissions": ["tabs", "webNavigation", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "ext-jptax_icon_128x128.png"
  },
  "icons": {
    "16": "ext-jptax_icon_16x16.png",
    "32": "ext-jptax_icon_32x32.png",
    "64": "ext-jptax_icon_64x64.png",
    "128": "ext-jptax_icon_128x128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}